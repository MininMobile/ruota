{
	"name": "ruota",
	"displayName": "Ruota",
	"description": "Adds Ruota language support for Code.",
	"version": "1.5.1",
	"publisher": "minin",
	"icon": "Logo.png",
	"engines": {
		"vscode": "^1.23.0"
	},
	"categories": [
		"Programming Languages",
		"Other"
	],
	"activationEvents": [
		"onCommand:ruota.run"
	],
	"main": "./extension",
	"contributes": {
		"commands": [
			{
				"command": "ruota.run",
				"title": "Ruota: Run File"
			}
		],
		"languages": [
			{
				"id": "ruo",
				"aliases": [
					"Ruota",
					"ruota"
				],
				"extensions": [
					".ruo"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "ruo",
				"scopeName": "source.ruota",
				"path": "./syntaxes/ruo.tmLanguage"
			}
		]
	},
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/Ruota-Lang/Ruota/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Ruota-Lang/Ruota.git"
	},
	"homepage": "https://ruota-lang.github.io/Ruota/",
	"__metadata": {
		"id": "816fa68a-dce1-48a5-9bb2-dd18593889f8",
		"publisherDisplayName": "Minin",
		"publisherId": "6cc86b95-7a38-48dd-9eb7-f5933e6a57d5"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^7.0.43",
		"eslint": "^4.11.0",
		"typescript": "^2.6.1",
		"vscode": "^1.1.29"
	}
}
